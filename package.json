{
  "name": "todo-list",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc -b",
    "start": "node .",
    "start:dev": "cross-env NODE_ENV=development env-cmd ts-node-dev --rs --respawn --transpile-only src/bin/www",
    "start:debug": "cross-env DEBUG=blog-sequelize:* npm run start:dev",
    "prettify": "prettier -w \"{config,db,public,src}/**/*.{md,html,css,json,js,ts}\"",
    "seq": "env-cmd sequelize"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
